#!name=TikTok JP
#!desc=解锁 TikTok 日本 地区
#!openUrl=https://github.com/Semporia/TikTok-Unlock
#!author=Semporia
#!homepage=https://github.com/Semporia/TikTok-Unlock
#!icon= https://raw.githubusercontent.com/Semporia/Hand-Painted-icon/master/Social_Media/TikTok.png

[Rule]
DOMAIN-SUFFIX,byteoversea.com,JP日本
DOMAIN-SUFFIX,ibytedtos.com,JP日本
DOMAIN-SUFFIX,ipstatp.com,JP日本
DOMAIN-SUFFIX,muscdn.com,JP日本
DOMAIN-SUFFIX,musical.ly,JP日本
DOMAIN-SUFFIX,tik-tokapi.com,JP日本
DOMAIN-SUFFIX,tiktok.com,JP日本
DOMAIN-SUFFIX,tiktokcdn.com,JP日本
DOMAIN-SUFFIX,tiktokv.com,JP日本
DOMAIN-KEYWORD,-tiktokcdn-com,JP日本
USER-AGENT,tiktok*,JP日本

[URL Rewrite]
(?<=_region=)CN(?=&) JP 307
(?<=&mcc_mnc=)4 2 307
^(https?:\/\/(tnc|dm)[\w-]+\.\w+\.com\/.+)(\?)(.+) $1$4 302
(?<=\d\/\?\w{7}_\w{4}=)1[6-9]..(?=.?.?&) 18.4 307

[MITM]
hostname = *.tiktokv.com,*.byteoversea.com,*.tik-tokapi.com
